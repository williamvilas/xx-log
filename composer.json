{
  "name": "vilasboas/log-formatter",
  "autoload": {
    "psr-4": {
      "LogFormatter\\": "src/"
    }
  },
  "authors": [
    {
      "name": "William Boas",
      "email": "williamhenriquevilasboas@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": ">=8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.68",
    "laravel/framework": "^10.0",
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "lint": "vendor/bin/php-cs-fixer fix -v --using-cache no src tests",
    "unit-test": "vendor/bin/phpunit --testdox --coverage-text"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "0.2.5"
}
